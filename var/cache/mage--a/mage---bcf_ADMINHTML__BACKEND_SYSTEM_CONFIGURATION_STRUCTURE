                        <a href="https:/www.paypal.com/webapps/mpp/promotional-financing" target="_blank">Learn More</a>";s:11:"config_path";s:33:"payment/paypal_express_bml/active";s:12:"source_model";s:40:"Magento\Config\Model\Config\Source\Yesno";s:14:"frontend_model";s:64:"Magento\Paypal\Block\Adminhtml\System\Config\Field\Enable\BmlApi";s:8:"requires";a:1:{s:5:"field";a:1:{s:2:"id";s:23:"enable_express_checkout";}}s:12:"_elementType";s:5:"field";s:4:"path";s:84:"payment_nz/paypal_payment_solutions/express_checkout_other/express_checkout_required";}s:13:"advertise_bml";a:10:{s:2:"id";s:13:"advertise_bml";s:9:"translate";s:13:"label comment";s:13:"showInDefault";s:1:"0";s:13:"showInWebsite";s:1:"0";s:9:"sortOrder";s:2:"30";s:5:"label";s:23:"Advertise PayPal Credit";s:7:"comment";s:895:"<a href="https:/financing.paypal.com/ppfinportal/content/whyUseFinancing" target="_blank">Why Advertise Financing?</a><br/>
                        <strong>Give your sales a boost when you advertise financing.</strong><br/>PayPal helps turn browsers into buyers with financing
